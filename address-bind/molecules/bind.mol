array Uint64 [byte; 8];
array Byte32 [byte; 32];
vector Bytes <byte>;

table Script {
    code_hash:      Byte32,
    hash_type:      byte,
    args:           Bytes,
}

table BindInfo {
    from: Script,
    to: Script,
    timestamp: Uint64,
}

table BindInfoWithSig {
    bind_info: BindInfo,
    sig: Bytes,
}
